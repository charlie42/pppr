<p id="notice"><%= notice %></p>

<h1>Listing Treatment Factors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @treatment_factors.each do |treatment_factor| %>
      <tr>
        <td><%= treatment_factor.name %></td>
        <td><%= link_to 'Show', treatment_factor %></td>
        <td><%= link_to 'Edit', edit_treatment_factor_path(treatment_factor) %></td>
        <td><%= link_to 'Destroy', treatment_factor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Treatment factor', new_treatment_factor_path %>
