<p id="notice"><%= notice %></p>

<h1>Listing Visits</h1>

<table>
  <thead>
    <tr>
      <th>From</th>
      <th>Date</th>
      <th>Complaints</th>
      <th>Anamnesis</th>
      <th>Allerg</th>
      <th>General state</th>
      <th>Diagnosis</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visits.each do |visit| %>
      <tr>
        <td><%= visit.from %></td>
        <td><%= visit.date %></td>
        <td><%= visit.complaints %></td>
        <td><%= visit.anamnesis %></td>
        <td><%= visit.allerg %></td>
        <td><%= visit.general_state %></td>
        <td><%= visit.diagnosis %></td>
        <td><%= link_to 'Show', visit %></td>
        <td><%= link_to 'Edit', edit_visit_path(visit) %></td>
        <td><%= link_to 'Destroy', visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visit', new_visit_path %>
